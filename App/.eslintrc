{
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": { "sourceType": "module" },
    //"parser": "babel-eslint",
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": 0,
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "no-console": 0,
        "no-unused-vars": ["error", { "vars": "local", "args": "none" }]
    }
}